#include <stdio.h>
#include <string.h>

void Palindrom_tester (char* word)
{
    int n = strlen(word)-1;
    for (int i=0; i!=(strlen(word)/2); i++) {
        if (word[i] != word [n]) {
            printf("No");
            break;
        }
    printf("Yes");
    }
}

int main()
{
    char word[50];
    printf("Insert a tested word \n");
    scanf("%s", word);
    //printf("%s", word);
    Palindrom_tester(word);
}
